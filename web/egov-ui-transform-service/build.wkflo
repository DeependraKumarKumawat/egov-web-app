def build(path, ci_image) {
    stage("Build") {
        docker.image("node:8.12-alpine").inside {
            sh "cd ${path}/rainmaker && cd packages/egov-ui-transform-service &&  npm install && cp -r node_modules src  ../../../egov-ui-transform-service/ && ls -al ../../../egov-ui-transform-service/";
        }
    }
}

return this;


