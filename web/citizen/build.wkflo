def build(path, ci_image) {
    stage("Build") {
        docker.image("egovio/ci-react:v8.4.0-lerna-v2").inside {
            sh "cd ${path}/rainmaker && rm -rf node_modules && npm i babel-cli && npm run build-css-citizen && npm run transpile-employee-modules && npm run transpile-citizen-modules  && lerna bootstrap && cd packages/citizen && npm run build && cp -r build ../../../citizen/";
        }
    }
}

return this;
