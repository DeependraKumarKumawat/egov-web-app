def build(path, ci_image) {
    stage("Build") {
        docker.image("egovio/ci-react:v8.15.0-alpine").inside {
            sh "cd ${path}/rainmaker && yarn config set registry http://egov-micro-dev.egovernments.org/verdaccio &&  yarn run egov-bootstrap && cd packages/employee && yarn run build && cp -r build ../../../employee/";
        }
    }
}

return this;
